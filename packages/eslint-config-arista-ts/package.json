{
  "name": "eslint-config-arista-ts",
  "version": "1.1.0",
  "description": "Arista's eslint config for TypeScript code",
  "author": "extensions@arista.com",
  "homepage": "http://www.arista.com",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "check": "npm run lint && npm run prettier-check",
    "lint": "eslint --max-warnings 0 *.js",
    "prepare": "npm run check",
    "prettier-check": "prettier -c *.js",
    "prettier-fix": "prettier --write *.js"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "arista",
    "typescript",
    "styleguide"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint-plugin-import": "2.20.2"
  },
  "devDependencies": {
    "@arista/prettier-config": "file:../prettier-config",
    "eslint-plugin-arista": "file:../eslint-plugin-arista"
  }
}
