{
  "name": "eslint-config-arista-ts",
  "version": "1.2.0",
  "description": "Arista's eslint config for TypeScript code",
  "author": "extensions@arista.com",
  "homepage": "http://www.arista.com",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "check": "npm run lint && npm run prettier-check",
    "lint": "eslint --max-warnings 0 *.js",
    "prepare": "npm run check",
    "prettier-check": "prettier -c *.js",
    "prettier-fix": "prettier --write *.js"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "arista",
    "typescript",
    "styleguide"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "eslint-plugin-import": "2.22.1"
  },
  "devDependencies": {
    "@arista/prettier-config": "1.1.3",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-arista-js": "1.1.29",
    "eslint-plugin-arista": "0.1.38",
    "eslint-plugin-flowtype": "5.2.0",
    "prettier": "2.1.2",
    "typescript": "4.0.3"
  }
}
