{
  "name": "eslint-config-arista-react",
  "version": "0.3.20",
  "description": "Arista's eslint config for React/JSX code",
  "author": "extensions@arista.com",
  "homepage": "http://www.arista.com",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "check": "npm run lint && npm run prettier-check",
    "lint": "eslint --max-warnings 0 *.js",
    "prepare": "npm run check",
    "prettier-check": "prettier -c *.js",
    "prettier-fix": "prettier --write *.js"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "arista",
    "javascript",
    "styleguide"
  ],
  "dependencies": {
    "eslint-config-arista-js": "1.1.25"
  },
  "devDependencies": {
    "@arista/prettier-config": "1.1.3",
    "babel-eslint": "10.1.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-arista": "0.1.34",
    "eslint-plugin-flowtype": "5.1.0",
    "eslint-plugin-import": "2.20.2",
    "prettier": "2.0.5"
  }
}
