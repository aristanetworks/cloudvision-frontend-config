  "description": "Arista's eslint config for React/JSX code",
  "author": "extensions@arista.com",
  "homepage": "https://github.com/aristanetworks/cloudvision-frontend-config/tree/trunk/packages/eslint-config-arista-react",
  "repository": {
    "type": "git",
