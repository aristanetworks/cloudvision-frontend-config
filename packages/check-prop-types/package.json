{
  "name": "@arista/check-prop-types",
  "version": "15.9.0",
  "description": "Custom fork of the mainline PropTypes.checkPropTypes",
  "author": "geiger-dev@arista.com",
  "homepage": "http://www.arista.com",
  "license": "MIT",
  "main": "lib/check-prop-types.js",
  "unpkg": "dist/check-prop-types.js",
  "module": "es/check-prop-types.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:commonjs && npm run build:es && npm run build:umd:min && npm run build:umd && npm run docs && npm run docs:md && npm run build:types",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/check-prop-types.js",
    "build:es": "cross-env NODE_ENV=es rollup -c -o es/check-prop-types.js",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig-build.json -outDir ./typings",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/check-prop-types.min.js",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/check-prop-types.js",
    "check": "npm run lint && npm run type-check && npm run prettier-check",
    "clean": "rimraf dist docs es lib typings",
    "docs": "typedoc --options typedoc.json --plugin typedoc-neo-theme --theme node_modules/typedoc-neo-theme/bin/default --out docs/html",
    "docs:md": "typedoc --options typedoc.json --plugin typedoc-plugin-markdown --theme markdown --out docs/md --hideSources",
    "lint": "eslint --max-warnings 0 --ext .js,.ts *.js src/**/*.ts",
    "prepare": "npm run check && npm run build",
    "prettier-check": "prettier -c *.js src/**/*.ts",
    "prettier-fix": "prettier --write *.js src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "files": [
    "dist",
    "es",
    "lib",
    "src",
    "typings"
  ],
  "dependencies": {
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "@arista/prettier-config": "1.1.2",
    "@rollup/plugin-commonjs": "11.1.0",
    "@rollup/plugin-node-resolve": "7.1.3",
    "@rollup/plugin-replace": "2.3.1",
    "@rollup/plugin-typescript": "4.1.1",
    "@types/prop-types": "15.7.3",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "babel-eslint": "10.1.0",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-arista-js": "1.1.18",
    "eslint-config-arista-ts": "1.1.0",
    "eslint-plugin-arista": "0.1.30",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.2",
    "prettier": "2.0.2",
    "rollup": "1.31.1",
    "rollup-plugin-terser": "5.3.0",
    "typedoc": "0.17.4",
    "typedoc-neo-theme": "1.0.8",
    "typedoc-plugin-markdown": "2.2.17",
    "typescript": "3.8.3"
  }
}
